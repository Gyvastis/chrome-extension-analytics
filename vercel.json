{
  "version": 2,
  "name": "chrome-extension-analytics",
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "frontend/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "@chrome_analytics_api_url"
  },
  "regions": ["iad1"],
  "functions": {
    "frontend/src/app/**": {
      "runtime": "nodejs18.x"
    }
  }
}